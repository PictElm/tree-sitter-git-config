{
  "name": "git_config",
  "rules": {
    "source": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "section"
      }
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "section_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "variable"
          }
        }
      ]
    },
    "subsection": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "subsection_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "variable"
          }
        }
      ]
    },
    "section_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "subsection_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "section_name"
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "subsection_name"
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "section_name": {
      "type": "PATTERN",
      "value": "[\\w\\.]"
    },
    "subsection_name": {
      "type": "PATTERN",
      "value": "[^\\r\\n\\x00]"
    },
    "variable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "name"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "SYMBOL",
              "name": "value"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "name"
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[a-zA-Z][\\w\\-]"
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_boolean"
        },
        {
          "type": "SYMBOL",
          "name": "color"
        },
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "_boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "true"
              },
              {
                "type": "STRING",
                "value": "yes"
              },
              {
                "type": "STRING",
                "value": "on"
              },
              {
                "type": "STRING",
                "value": "1"
              }
            ]
          },
          "named": true,
          "value": "true"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "false"
              },
              {
                "type": "STRING",
                "value": "no"
              },
              {
                "type": "STRING",
                "value": "off"
              },
              {
                "type": "STRING",
                "value": "0"
              },
              {
                "type": "STRING",
                "value": "\"\""
              }
            ]
          },
          "named": true,
          "value": "false"
        }
      ]
    },
    "true": {
      "type": "STRING",
      "value": "true"
    },
    "false": {
      "type": "STRING",
      "value": "false"
    },
    "color": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "FIELD",
          "name": "foreground",
          "content": {
            "type": "SYMBOL",
            "name": "_color"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "FIELD",
                  "name": "background",
                  "content": {
                    "type": "SYMBOL",
                    "name": "_color"
                  }
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "attribute"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_color": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "25[0-5]|2[0-4]\\d|1\\d\\d|\\d\\d|\\d"
        },
        {
          "type": "PATTERN",
          "value": "#[0-9a-fA-F]{6}"
        },
        {
          "type": "STRING",
          "value": "normal"
        },
        {
          "type": "STRING",
          "value": "black"
        },
        {
          "type": "STRING",
          "value": "red"
        },
        {
          "type": "STRING",
          "value": "green"
        },
        {
          "type": "STRING",
          "value": "yellow"
        },
        {
          "type": "STRING",
          "value": "blue"
        },
        {
          "type": "STRING",
          "value": "magenta"
        },
        {
          "type": "STRING",
          "value": "cyan"
        },
        {
          "type": "STRING",
          "value": "white"
        },
        {
          "type": "STRING",
          "value": "brightblack"
        },
        {
          "type": "STRING",
          "value": "brightred"
        },
        {
          "type": "STRING",
          "value": "brightgreen"
        },
        {
          "type": "STRING",
          "value": "brightyellow"
        },
        {
          "type": "STRING",
          "value": "brightblue"
        },
        {
          "type": "STRING",
          "value": "brightmagenta"
        },
        {
          "type": "STRING",
          "value": "brightcyan"
        },
        {
          "type": "STRING",
          "value": "brightwhite"
        }
      ]
    },
    "attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "bold"
        },
        {
          "type": "STRING",
          "value": "dim"
        },
        {
          "type": "STRING",
          "value": "ul"
        },
        {
          "type": "STRING",
          "value": "blink"
        },
        {
          "type": "STRING",
          "value": "reverse"
        },
        {
          "type": "STRING",
          "value": "italic"
        },
        {
          "type": "STRING",
          "value": "strike"
        },
        {
          "type": "STRING",
          "value": "no-bold"
        },
        {
          "type": "STRING",
          "value": "no-dim"
        },
        {
          "type": "STRING",
          "value": "no-ul"
        },
        {
          "type": "STRING",
          "value": "no-blink"
        },
        {
          "type": "STRING",
          "value": "no-reverse"
        },
        {
          "type": "STRING",
          "value": "no-italic"
        },
        {
          "type": "STRING",
          "value": "no-strike"
        }
      ]
    },
    "integer": {
      "type": "PATTERN",
      "value": "\\d+[kmgtpezyKMGTPEZY]?"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\\"\\\\]"
              },
              {
                "type": "SYMBOL",
                "name": "_escape_sequence"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "_escape_sequence": {
      "type": "PATTERN",
      "value": "\\\\([btnfr\"\\\\]|u[0-9a-fA-F]{4}|U[0-9a-fA-F]{8})"
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ";"
            },
            {
              "type": "STRING",
              "value": "#"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "[^\\r\\n]"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": " \\t\\f\\v"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

